{
    "name": "axeldotdev/laravel-airtable",
    "description": "laravel-airtable allows you to import your Airtable data into your Laravel database.",
    "keywords": [
        "axeldotdev",
        "laravel-airtable"
    ],
    "homepage": "https://github.com/axeldotdev/laravel-airtable",
    "license": "MIT",
    "authors": [
        {
            "name": "axeldotdev",
            "email": "axelcharpentier0@icloud.com",
            "role": "Developer",
            "homepage": "https://github.com/axeldotdev"
        }
    ],
    "readme": "https://github.com/axeldotdev/laravel-airtable/blob/main/README.md",
    "support": {
        "email": "axelcharpentier0@icloud.com",
        "source": "https://github.com/axeldotdev/laravel-airtable",
        "issues": "https://github.com/axeldotdev/laravel-airtable/issues",
        "docs": "https://github.com/axeldotdev/laravel-airtable/wiki",
        "chat": "https://github.com/axeldotdev/laravel-airtable/discussions"
    },
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.11",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "AxelDotDev\\LaravelAirtable\\": "src",
            "AxelDotDev\\LaravelAirtable\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AxelDotDev\\LaravelAirtable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AxelDotDev\\LaravelAirtable\\LaravelAirtableServiceProvider"
            ],
            "aliases": {
                "Airtable": "AxelDotDev\\LaravelAirtable\\Facades\\Airtable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
